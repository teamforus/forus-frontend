- var src_prefix = (qdt_c.platform.env_data.html5ModeEnabled?qdt_c.platform.env_data.html5Mode.basePath:'');

//- libs bundle
script(src=src_prefix + "./assets/dist/bundle/js/bundle.min" + qdt_c.append_assets + ".js")

//- custom script
script(src=src_prefix + "./assets/js/app.min" + qdt_c.append_assets + ".js")

//- google maps
if (qdt_c.platform.env_data.google_maps_api_key)
    script.
        window.mapsCallback = () => window.mapsLoaded = true
    script(
        type="text/javascript" 
        src="https://maps.googleapis.com/maps/api/js?libraries=places&language=nl&callback=mapsCallback&key=" + qdt_c.platform.env_data.google_maps_api_key
        async defer)